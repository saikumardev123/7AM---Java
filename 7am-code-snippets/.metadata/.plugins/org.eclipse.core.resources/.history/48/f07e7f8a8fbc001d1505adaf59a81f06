
public class LocalVariablesDemo {
	

	  public static void m1() {
		     // local variable
		    System.out.println(a);	
		     if(a < 200) {
		    	int a = 200;
		     }
		     System.out.println(a);
		     
		     int a=100;
	  }
	 
	public static void main(String[] args) {
		LocalVariablesDemo.m1();
	}
	
}
