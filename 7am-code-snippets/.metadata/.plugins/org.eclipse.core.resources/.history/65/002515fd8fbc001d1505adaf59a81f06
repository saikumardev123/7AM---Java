
public class LocalVariablesDemo {
	

	  public static void m1() {
		 
		     if(20 < 200) {
		    	int a = 200; // blocked scoped variable
		    	System.out.println(a);
		     }
		     int a=0;
		     System.out.println(a); // ? 
	  }
	 
	public static void main(String[] args) {
		LocalVariablesDemo.m1();
	}
	
}
