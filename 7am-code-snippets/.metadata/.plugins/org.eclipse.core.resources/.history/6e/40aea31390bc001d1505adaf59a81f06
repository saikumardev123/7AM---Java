
public class LocalVariablesDemo {
	

	  public static int m1() {
		 
		     if(20 < 200) {
		    	int a = 200 * 10; // blocked scoped variable
		    	System.out.println(a);
		    	return a;
		    	
		     }
		     int a=0;
		     System.out.println(a); // ? 
	  }
	 
	public static void main(String[] args) {
		LocalVariablesDemo.m1();
	}
	
}
